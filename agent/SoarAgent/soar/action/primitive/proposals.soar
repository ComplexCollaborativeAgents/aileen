sp {action*propose*goto
    (state <s>    ^name aileen
                  ^world.objects.object <obj>)
    (<obj>    ^object_type Apple)
    
-->
    (<s>    ^operator <op> +)
    (<op>    ^name go-to
             ^object <obj>)
    } 

sp {action*apply*goto
    (state <s>    ^name aileen
                  ^operator <op> +
                  ^io.output-link <ol>)
    (<op>    ^name go-to
             ^object <obj>)
    (<obj>    ^position <p>)
    (<p>    ^x <x>
            ^y <y>
            ^z <z>)
-->
    (<ol>    ^action <act>)
    (<act>    ^name teleport
              ^position <pose>)
    (<pose>    ^x (- <x> 0.5)
                ^y <y>
                ^z (- <z> 0.5))
    }
    